{
  "name": "css-fade",
  "version": "1.0.5",
  "type": "module",
  "homepage": "https://github.com/dan-smt/css-fade",
  "repository": {
    "type": "git",
    "url": "https://github.com/dan-smt/css-fade",
    "directory": "packages/core"
  },
  "description": "Fade with css masks instead of adding extra linear gradient elements to the DOM. Works with TailwindCSS, UnoCSS, and CSS.",
  "keywords": [
    "fade",
    "mask",
    "gradient",
    "overlay"
  ],
  "bugs": {
    "url": "https://github.com/dan-smt/css-fade/issues"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./unocss": {
      "types": "./dist/unocss/index.d.ts",
      "import": "./dist/unocss/index.mjs",
      "require": "./dist/unocss/index.cjs"
    },
    "./tailwindcss": {
      "types": "./dist/tailwindcss/index.d.ts",
      "import": "./dist/tailwindcss/index.mjs",
      "require": "./dist/tailwindcss/index.cjs"
    },
    "./css": "./dist/css/index.css",
    "./css/utilities": "./dist/css/utilities.css"
  },
  "scripts": {
    "build": "rimraf ./dist && unbuild"
  },
  "devDependencies": {
    "@types/postcss-js": "^4.0.4",
    "postcss": "^8.4.39",
    "postcss-js": "^4.0.1",
    "rimraf": "^5.0.7",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "unocss": "^0.61.0",
    "unbuild": "3.0.0-rc.11"
  }
}
